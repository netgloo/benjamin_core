{
  "name": "netgloo/benjamin_core",
  "type": "library",
  "description": "Benjamin's core",
  "keywords": [
    "netgloo",
    "benjamin",
    "benjamin_core"
  ],
  "homepage": "https://github.com/netgloo/benjamin_core",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Zanelli",
      "email": "andrea.zanelli@netgloo.com",
      "homepage": "http://netgloo.com/en",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.1",
    "php" : ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "netgloo\\benjamin_core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "netgloo\\benjamin_core\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
